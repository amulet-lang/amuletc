const main = () do
    match "Hello, world!" with
        | "Hello, " <> rest do
            @println(rest)
        | _ => ()
    end

    print_array(uint, &.{1, 2, 3, 4, 5})
end

const print_array = (comptime 'a: type, slice: []'a) do
    match slice with
        | [head, ..tail] do
            @print("#{head}, ")
            print_array('a, tail)
        | _ => @println("")
    end
end
