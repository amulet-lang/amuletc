const Vector = record {
    pub const t = record {
        x: isize,
        y: isize
    }

    pub const new -> t = (x, y: isize) do
        t.{ x, y }
    end

    pub const t.add -> t = (other: t) do
        t.{
            x = self.x + other.x,
            y = self.y + other.y
        }
    end
}

const main -> @result(unit) = () do
    let a = Vector.new(1, 2)
    let b = Vector.new(3, 4)
    let c = a.add(b)
end
