const {Buffer, Vector} = @use("idk");

const main -> @result(unit) = () do
    var buffer = Buffer.init()
    Vector.new(1, 2)
        |> .add(.new(3, 4))
        |> .write(buffer.writer())

    c.write(buffer.writer())
    @expectEq(buffer.buf.get(), 4).?
    @expectEq(buffer.buf.get(), 5).?
end
