/*
 * Program to calculate fibonacci numbers
 */

const std = @import("std")
const string = std.string

const fibonacci -> usize = (n: usize) do
    if n <= 1 do
        1
    else do
        fibonacci(n-1) + fibonacci(n-2)
    end
end

const main -> @result(unit) = () do
    let n = std.io.stdin().gets()
        |> string.parse ~into: usize
        |> .!

    let value = fibonacci(n)

    @printf("Fibonacci number {} is {}\n", {n, value})
end
