/*
 * Program to calculate fibonacci numbers
 */

/* Loads the modules into variables, could also be done with std */
const fib = gh.import("fib.gh");

/* Exposes bindings from modules */
use .{io, parse, string} from "std";

/* Named function */
public fn main = () -> Result((), any) {
    let n = io.gets()
        |> string.parse(~into=int) 
        |> (!);

    let value = fib.fibonacci(n);

    io.println("Fibonacci number {} is {}", n, value);

    return ();
}
