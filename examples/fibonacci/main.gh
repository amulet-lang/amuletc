/*
 * Program to calculate fibonacci numbers
 */

/* Exposes bindings from modules */
use std.(io.*, parse, string);
use fib;

/* Named function */
def main = () -> Result((), any) {
    let n = gets()
        |> string.parse(~into: int) 
        |> (!);

    let value = fib.fibonacci(n);

    io.println("Fibonacci number {} is {}", n, value);

    return ();
}
