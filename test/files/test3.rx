const std = rex.import("std");

const name = "Foo";
const year = 2023;
const multi = "| Hello,
             | {s}!
             | From {d}
             |";

// Formats greeting with name and year
const greet = (name: string, year: int) => string { |&multi|
    return std.fmt.string(multi, name, year);
};

greet(name, year) 
    |> std.io.println();
