const Result -> type = (
    comptime success: type
) do
    union {
        Ok(success),
        Err(any Error)
    }
end

const parseError = struct {
    ...
}

impl Error(parseError) = {
    ...
}

const parse -> Result(int) = (&str: string) do
    if ... {
        return Err <| parseError.{};
    }
end

const main -> Result(()) = () do
    let str = "Hello";
    let result = parse(str)?;
    println("result: {d}", {result});

    Ok(())
end
