/*
 * Program to calculate fibonacci numbers
 */

const string = ruka.import("std").string
const fib = ruka.import("fib")

const main -> Result(unit) = () {
    let n = gets()
        |> string.parse() ~into: int
        |> (?)

    let value = fib.fibonacci(n)

    ruka.println("Fibonacci number {} is {}", {n, value})

    () |> Ok
}
