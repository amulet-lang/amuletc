fn main = () {
    match "Hello, world!" with
        "Hello, " <> rest {
            @println(rest)
        }
        _ => ()
    end

    printArray(uint, &[1, 2, 3, 4, 5])
}

fn printArray = (comptime 'a: type, slice: []'a) {
    match &slice {
        [head, ..tail] {
            @print("{} ", {head})
            printArray('a, tail)
        }
        _ => @println("")
    }
}
