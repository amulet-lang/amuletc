fn main = () do
    match "Hello, world!" with
        "Hello, " <> rest do
            @println(rest)
        _ => ()
    end

    printArray(uint, &[1, 2, 3, 4, 5])
end

fn printArray = (comptime 'a: type, slice: []'a) do
    match &slice with
        [head, ..tail] do
            @print("{} ", {head})
            printArray('a, tail)
        _ => @println("")
    end
end
