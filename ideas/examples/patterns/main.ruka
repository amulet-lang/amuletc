fn main = () do
    match "Hello, world!" with
        | "Hello, " <> world do
            @println(world)
        | _ => ()
    end

    printArray(uint, &[1, 2, 3, 4, 5])
end

fn printArray = (comptime 'a: type, slice: []'a) do
    match &data with
        | [head, ..tail] do
            @print("{} ", {head})
            printArray('a, tail)
        | _ do 
            @print("\n")
        end
    end
end
